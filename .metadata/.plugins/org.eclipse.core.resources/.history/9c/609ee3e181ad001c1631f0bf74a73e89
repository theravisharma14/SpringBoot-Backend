package com.example.bean;

//bean class

public class User {
	private int id;
	private String name;
	
	//non-arg constructor
	public User() {}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}

}
