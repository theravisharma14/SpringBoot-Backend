package com.example.demo.user;

import org.springframework.beans.factory.annotation.Value;
import org.springframework.stereotype.Component;

@Component
public class User {

@Value("1")	
private int id;


@Value("Ravi")
private String name;


@Override
public String toString() {
	return "User [id=" + id + ", name=" + name + "]";
}
	
}
